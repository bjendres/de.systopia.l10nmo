<?xml version="1.0"?>
<extension key="de.systopia.l10nmo" type="module">
  <file>l10nmo</file>
  <name>Custom Translation Files</name>
  <description>This extension allows you to provide custom translation files.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.l10nmo</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.l10nmo</url>
    <url desc="Support">https://github.com/systopia/de.systopia.l10nmo/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-08-27</releaseDate>
  <version>0.1</version>
  <develStage>experimental</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <requires>
    <ext>org.civicrm.l10nx</ext>
  </requires>
  <comments/>
  <civix>
    <namespace>CRM/L10nmo</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
